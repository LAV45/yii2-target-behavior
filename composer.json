{
    "name": "lav45/yii2-target-behavior",
    "description": "This extension allows you to link between multiple objects.",
    "keywords": [
        "yii2",
        "extension",
        "behavior",
        "target",
        "many to many"
    ],
    "homepage": "https://github.com/lav45/yii2-target-behavior",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alexey Loban",
            "email": "lav451@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "lav45\\behavior\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    }
}
